<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="center" prefHeight="282.0" prefWidth="221.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.NickController">
   <stylesheets>
      <URL value="@/css/style.css" />
   </stylesheets>
   <children>
      <TextField fx:id="loginField" promptText="login" HBox.hgrow="ALWAYS" />
      <TextField fx:id="passwordField" promptText="password" HBox.hgrow="ALWAYS" />
      <TextField fx:id="newNicknameField" promptText="New nickname" />
      <TextArea fx:id="textArea" editable="false" />
      <Button onAction="#tryToChangeNick" text="Change" />
   </children>
</VBox>
